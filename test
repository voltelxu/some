# -*- coding:utf-8 -*-
import re
f = open('kotext', 'r')
o = open('cleartext','w')
i = 1
for l in f:
    #s = l.replace('\n',' ')
    s = re.sub('[A-Za-z]+',' ',l)
    s = re.sub('[0-9]+','',s)
    s = re.sub('[(]+',' ',s)
    s = re.sub('[)]+',' ',s)
    s = re.sub('[.]+',' ',s)
    s = re.sub('[,]+',' ',s)
    s = re.sub('[\']+',' ',s)
    s = re.sub('["]+',' ',s)
    s = re.sub('[\/]+',' ',s)
    s = re.sub('[[]+',' ',s)
    s = re.sub('[]]+',' ',s)
    s = re.sub('[@]+',' ',s)
    s = re.sub('[&]+',' ',s)
    s = re.sub('[-]',' ',s)
    s = re.sub('[=]',' ',s)
    s = re.sub('[:]',' ',s)
    s = re.sub('[;]',' ',s)
    s = re.sub('[?]',' ',s)
    s = re.sub('[%]',' ',s)
    s = re.sub('[+]',' ',s)
    s = re.sub('[*]',' ',s)
    s = re.sub('[1-9]+','',s)
    o.write(s)
    i = i + 1
    print(i)
o.close()
f.close()
